{
    "name": "bashmohandes7/laravel-zoom-package",
    "description": "sample package for laravel applications to integrate with Zoom",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "7.7.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Diaa Abdallah",
            "email": "diaaemad1@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bashmohandes7\\ZoomService\\":"src/"
        }
    },
    "files": [
        "src/helpers.php"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Bashmohandes7\\ZoomService\\ZoomServiceProvider"
            ],
            "aliases": {
                "Zoom": "Bashmohandes7\\ZoomService\\Zoom"
            }
        }
    }
}
