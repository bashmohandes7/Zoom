{
    "name": "bashmohandes7/laravel-zoom-package",
    "description": "sample package for laravel applications to integrate with Zoom",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "7.7.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Diaa Abdallah",
            "email": "diaaemad1@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bashmohandes7\\Zoom\\":"src/"
        },
        "files": [
            "src/helpers.php"
        ],
        "extra": {
            "laravel": {
                "providers": [
                    "Bashmohandes7\\Zoom\\ZoomServiceProvider"
                ],
                "aliases": {
                    "Zoom": "Bashmohandes7\\Zoom\\Facade"
                }
            }
        }
    }
}
